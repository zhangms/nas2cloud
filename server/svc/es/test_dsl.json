{
  "query": {
    "bool": {
      "filter": [
        {
          "term": {
            "Type": "FILE"
          }
        },
        {
          "terms": {
            "Bucket": [
              "Movies",
              "Pic",
              "_NO_BUCKET_"
            ]
          }
        },
        {
          "terms": {
            "Ext": [
              ".JPG",
              ".PNG",
              ".JPEG",
              ".BMP",
              ".GIF",
              ".MP4",
              ".MOV",
              ".MKV",
              ".3GP"
            ]
          }
        },
        {
          "range": {
            "ModTime": {
              "gte": "2012-01",
              "lt": "2023-02",
              "format": "yyyy-MM",
              "time_zone": "-08:00"
            }
          }
        }
      ]
    }
  },
  "track_total_hits": true
}